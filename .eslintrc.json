{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["airbnb-base"],  
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "plugins": ["react"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 2,
        "no-alert": 2
    }
}